		<script>document.getElementById("filtertype").value="<% echo -n $filtertype_select %>";</script>

		<td colspan="2"></td>
		<td colspan="7"><INPUT TYPE="SUBMIT" name="BUTTON" VALUE="ADD_FILTER"></td>
